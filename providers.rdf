<skos:Concept rdf:about="5db54f8a-9560-4c43-91ce-3f9fc9fde0a6"
   xml:base="https://gcmd.earthdata.nasa.gov/kms/concept/"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:skos="http://www.w3.org/2004/02/skos/core#">
   <skos:inScheme rdf:resource="https://gcmd.earthdata.nasa.gov/kms/concepts/concept_scheme/providers"/>
   <skos:prefLabel xml:lang="en">AAG</skos:prefLabel>
   <gcmd:altLabel gcmd:category="primary"
      gcmd:text="Association of American Geographers" xml:lang="en"/>
   <skos:definition xml:lang="en">The Association of American Geographers (AAG) is a nonprofit scientific and educational society founded in 1904. For 100 years the AAG has contributed to the advancement of geography. Its members from more than 60 countries share interests in the theory, methods, and practice of geography, which they cultivate through the AAG&amp;#039;s Annual Meeting, two scholarly journals (Annals of the Association of American Geographers and The Professional Geographer), and the monthly AAG Newsletter. The AAG promotes discussion among its members and with scholars in related fields, in part through the activities of its affinity groups and 53 specialty groups. The meetings and activities of our regional divisions provide the opportunity to network with colleagues near you.

Summary Provided By:

http://www.aag.org/Info/info.html</skos:definition>
   <gcmd:resource gcmd:type="PROVIDER" gcmd:url="http://www.aag.org/"/>
   <skos:broader rdf:resource="aef2fc80-def0-46a4-9259-9dc37e80ed08"/>
</skos:Concept>

<skos:Concept rdf:about="2a5cc97c-26b2-449b-bb62-6be1eed53586"
   xml:base="https://gcmd.earthdata.nasa.gov/kms/concept/"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:skos="http://www.w3.org/2004/02/skos/core#">
