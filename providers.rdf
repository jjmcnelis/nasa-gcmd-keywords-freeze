</skos:Concept>

<skos:Concept rdf:about="d8a75bb2-fa28-4373-b736-4a1a1b8b6e68"
   xml:base="https://gcmd.earthdata.nasa.gov/kms/concept/"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:skos="http://www.w3.org/2004/02/skos/core#">
   <skos:inScheme rdf:resource="https://gcmd.earthdata.nasa.gov/kms/concepts/concept_scheme/providers"/>
   <skos:prefLabel xml:lang="en">SCRIPPS/CALSPACE</skos:prefLabel>
   <gcmd:altLabel gcmd:category="primary"
      gcmd:text="California Space Institute, Scripps Institution of Oceanography" xml:lang="en"/>
   <skos:definition xml:lang="en">Earthguide is a web-based teacher's resource in earth and environmental science, oceanography, and related technologies including earth observation via satellite. Topic presentations stress an earth systems approach and highlight linkages between global and local systems. We also highlight ongoing research at Scripps Institution of Oceanography and often work in partnership with the Birch Aquarium at Scripps. Resources are made available through outreach efforts developed with researchers and regional schools. Resources include maps, data, images, animations, expedition coverage, web-based interactivities for students, and more. Our materials evolve with input from teachers and students like you.</skos:definition>
   <gcmd:resource gcmd:type="PROVIDER" gcmd:url="http://casgc.ucsd.edu/"/>
   <skos:broader rdf:resource="aef2fc80-def0-46a4-9259-9dc37e80ed08"/>
</skos:Concept>

