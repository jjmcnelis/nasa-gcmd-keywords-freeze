   <skos:prefLabel xml:lang="en">GEOOPTICS</skos:prefLabel>
   <gcmd:altLabel gcmd:category="primary" gcmd:text="GeoOptics" xml:lang="en"/>
   <skos:definition xml:lang="en">GeoOptics is an Environmental Earth Observation company that will deliver real-time data about our planet for daily operational applications and long term monitoring of the environment. GeoOptics was founded by working scientists, engineers, and data users in response to the growing urgent need for observing our planet and its environment – and the impacts that our Earth’s environment has on life on this planet. GeoOptics will deliver data and related products to users world wide from a variety of sensors in space and on earth with a wide range of applications.</skos:definition>
   <gcmd:resource gcmd:type="PROVIDER" gcmd:url="http://geooptics.com"/>
   <skos:broader rdf:resource="1bb6821d-8534-40ce-a6ee-7c88ce4d3c9f"/>
</skos:Concept>

<skos:Concept rdf:about="fc62e7af-25c9-4722-b214-7572aec61c46"
   xml:base="https://gcmd.earthdata.nasa.gov/kms/concept/"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:skos="http://www.w3.org/2004/02/skos/core#">
